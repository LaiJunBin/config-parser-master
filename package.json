{
  "name": "config-parser-master",
  "version": "0.1.1",
  "description": "This is a simple config parser that can read and write to a config file.",
  "repository": {
    "type": "git",
    "url": "https://github.com/LaiJunBin/config-parser-master.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup src/index.ts --minify --clean --sourcemap --watch",
    "start": "node --enable-source-maps dist/index.js",
    "build": "tsup src/index.ts --minify --clean --dts",
    "test": "vitest"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "config",
    "parser",
    "config-parser",
    "config-parser-master"
  ],
  "files": [
    "dist"
  ],
  "author": "Lai JunBin",
  "license": "MIT",
  "devDependencies": {
    "@babel/generator": "^7.22.5",
    "@babel/parser": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "^7.22.5",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  }
}
